public class Red extends Vietati {

    Red(int putere, int viteza) {
        this.putere = putere;
        this.viteza = viteza;
    }

    public int getPowerAttack() {
        return this.putere*this.viteza;
    }

    public int usePower() {
        return this.putere = this.putere * 10;
    }
}